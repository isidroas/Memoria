@article{tiltrotor-control,
   title = "Autonomous Hovering of a Noncyclic Tiltrotor UAV: Modeling,
   Control and Implementation",
   journal = "IFAC Proceedings Volumes",
   volume = "41",
   number = "2",
   pages = "803 - 808",
   year = "2008",
   note = "17th IFAC World Congress",
   issn = "1474-6670",
   doi = {10.3182/20080706-5-KR-1001.00138},
   url =
      "http://www.sciencedirect.com/science/article/pii/S1474667016390486",
   author = "A. Sanchez and J. Escareño and O. Garcia and R. Lozano",
   abstract = "The aim of this paper is to present a mini tilt-rotor
      unmanned aerial vehicle which is capable to perform hover flight.
      Unlike conventional full-scale tiltrotors, in our design we avoid the
      use of swashplate and we propose a simpler mechanical design which
      use only the tilting rotors to stabilize the vehicle dynamics. A
      detailed mathematical model is derived via the Newton-Euler
      formalism. A nonlinear control scheme, incorporating bounded smooth
      function, is obtained from the decoupled dynamics and applied to real
      prototype for controlling hover flight."
}
